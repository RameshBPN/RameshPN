    <webLinks>
        <fullName>actimizerdialer__Add_to_Call_Queue</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add to My Call Queue</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)}

var records = {!GETRECORDIDS($ObjectType.Campaign)};

if (!records || records.length == 0) {
  alert(&quot;{!$Label.actimizerdialer__NoRecordsSelected}&quot;);
}
else {
  sforce.apex.execute(
    &apos;actimizerdialer.CallListController&apos;,
    &apos;AddCampaign&apos;,
    { &quot;subjectType&quot;: &quot;Campaign&quot;, &quot;ids&quot;: records }, 
    function(result) {
      var count = result ? result[0] : 0;

      if (count &gt; 0) {
        if (count == 1) {
            window.alert(&quot;{!$Label.actimizerdialer__CampaignAdded_Singular}&quot;);
        }
        else {
            window.alert(count + &quot; {!$Label.actimizerdialer__CampaignAdded_Plural}&quot;);
        }
        var query = window.location.search.substring(1);
        var vars = query.split(&apos;&amp;&apos;);
        var retURL;
        for (var i = 0; i &lt; vars.length; i++) {
          var pair = vars[i].split(&apos;=&apos;);
          if (decodeURIComponent(pair[0]) == &apos;retURL&apos;) {
            retURL = decodeURIComponent(pair[1]);
            break;
          }
        }
        if(retURL) {
          window.location = retURL;
        }
        else {
          window.location.reload();
        }
      }
      else if (count == 0) {
        window.alert(&quot;{!$Label.actimizerdialer__CampaignsAlreadyInCallQueue}&quot;);
      }
      else if (count == -1) {
        window.alert(&quot;{!$Label.actimizerdialer__NoAccessActimizerCampaignParticipants}&quot;);
      }
    });
}</url>
    </webLinks>
