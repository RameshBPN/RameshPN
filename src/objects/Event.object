        <url>{!REQUIRESCRIPT(&quot;/support/console/26.0/integration.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 

var win_target = &apos;_top&apos;;
var win_Location = window.location.href;
var actmizer_Index = win_Location.indexOf(&apos;actimizerdialer&apos;);
if(actmizer_Index != -1)
{
  win_target = &apos;_blank&apos;;
}
else{
   win_target = &apos;_top&apos;; 
}
if({!ISPICKVAL( $User.Sales_Area1__c , &apos;1000&apos;)} || {!ISPICKVAL( $User.Sales_Area1__c , &apos;4000&apos;)}){ 
window.open(&quot;/00U/e?what_id={!Account.Id}&amp;evt12={!Account.Name}, {!Account.Postal_Address_Street__c} {!Account.Postal_Address_House_Number__c}, {!Account.Postal_Address_Postal_Code__c} {!Account.Postal_Address_City__c}&amp;retURL={!Account.Id}&quot;,win_target)
}
else{
window.open(&apos;/00U/e?what_id={!Account.Id}&amp;retURL={!Account.Id}&apos;,win_target)
}</url>
