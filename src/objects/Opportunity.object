        <fullName>actimizerdialer__Add_to_Call_List</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add to My Call Queue</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)}

var records = {!GETRECORDIDS($ObjectType.Opportunity)};

if (!records || records.length == 0) {
  window.alert(&quot;{!$Label.actimizerdialer__NoRecordsSelected}&quot;);
}
else {
  var opportunityUpdateable = false;
  var lockedByUpdateable = false;
  try {
    var d = sforce.connection.describeSObject(&apos;Opportunity&apos;);
    var opportunityUpdateable = d.updateable === &quot;true&quot;;

    if (opportunityUpdateable) {
      for(var i=d.fields.length-1; i&gt;=0; i--) {
        var f = d.fields[i];
        if (f.name == &apos;actimizerdialer__LockedBy__c&apos;) {
          lockedByUpdateable = f.updateable === &quot;true&quot;;
          break;
        }
      }
    }
  }
  catch(Exception) { }

  if (opportunityUpdateable &amp;&amp; lockedByUpdateable ) {
    sforce.apex.execute(
      &apos;actimizerdialer.CallListController&apos;,
      &apos;AddToCallList2&apos;,
      { &quot;subjectType&quot;: &quot;Opportunity&quot;, &quot;ids&quot;: records }, 
      function(result) {
        var count = result ? result[0] : 0;

        if (count &gt; 0) {
          if (count == 1) {
            window.alert(&quot;{!$Label.actimizerdialer__OpportunityAdded_Singular}&quot;);
          }
          else {
            window.alert(count + &quot; {!$Label.actimizerdialer__OpportunityAdded_Plural}&quot;);
          }
          var query = window.location.search.substring(1);
          var vars = query.split(&apos;&amp;&apos;);
          var retURL;
          for (var i = 0; i &lt; vars.length; i++) {
            var pair = vars[i].split(&apos;=&apos;);
            if (decodeURIComponent(pair[0]) == &apos;retURL&apos;) {
              retURL = decodeURIComponent(pair[1]);
              break;
            }
          }
          if(retURL) {
            window.location = retURL;
          }
          else {
            window.location.reload();
          }
        }
        else if (count == 0) {
          window.alert(&quot;{!$Label.actimizerdialer__OpportunitiesAlreadyInCallQueue}&quot;);
        }
        else if (count == -1) {
          window.alert(&quot;{!$Label.actimizerdialer__NoAccessOpportunities}&quot;);
        }
      });
  }
  else {
    window.alert(&quot;{!$Label.actimizerdialer__NoAccessOpportunities}&quot;);
  }
}</url>
    </webLinks>
