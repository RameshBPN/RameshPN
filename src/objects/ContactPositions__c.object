<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>List of contact positions codes (use name field to store the codes) to be imported by default when an account is enriched or created from Postnord Customer Master</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Country__c</fullName>
        <externalId>false</externalId>
        <label>Country</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Description__c</fullName>
        <description>The Name field contains the Contact Person code stored in PNCM. The description field should explain what the code represents, ex. 1234, Chief Executive Officer</description>
        <externalId>false</externalId>
        <inlineHelpText>The Name field contains the Contact Person code stored in PNCM. The description field should explain what the code represents, ex. 1234, Chief Executive Officer</inlineHelpText>
        <label>Description</label>
        <length>100</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Language__c</fullName>
        <externalId>false</externalId>
        <label>Language</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Required__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Required</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Contact Positions</label>
</CustomObject>
