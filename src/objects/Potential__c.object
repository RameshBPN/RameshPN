    <validationRules>
        <fullName>Account_Mandatory_on_Potential</fullName>
        <active>true</active>
        <description>Must have an account to be able to load a potential</description>
        <errorConditionFormula>ISBLANK( Customer__c ) &amp;&amp; $Setup.DisableAutomation__c.Disable_All_Validation_Rules__c = false</errorConditionFormula>
        <errorDisplayField>Customer__c</errorDisplayField>
        <errorMessage>You cannot load a potential without an account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_must_be_Legal_Ent_for_ME_and_LO</fullName>
        <active>true</active>
        <description>For all ME SE and Logistik (LO SE, LO NO, LO FI, LO FI DPD, LO DK) potentials, the related account must be of record type Legal Entity</description>
        <errorConditionFormula>(ISPICKVAL (Sales_Area__c, &apos;FO91&apos;) ||
ISPICKVAL (Sales_Area__c, &apos;FO92&apos;) ||
ISPICKVAL (Sales_Area__c, &apos;0630&apos;) ||
ISPICKVAL (Sales_Area__c, &apos;3670&apos;) ||
ISPICKVAL (Sales_Area__c, &apos;LO NO&apos;) ||
ISPICKVAL (Sales_Area__c, &apos;0540&apos;)) &amp;&amp;
Customer__r.RecordType.Name != &apos;Legal Entity&apos; &amp;&amp;
$Setup.DisableAutomation__c.Disable_All_Validation_Rules__c = false</errorConditionFormula>
        <errorMessage>Potential for ME SE or Logistik must be attached to a Legal Entity. The account you have tried to associate this Potential with is not a Legal Entity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_must_be_Work_Site_for_PDK_BE_LO</fullName>
        <active>true</active>
        <description>For all PDK BE and PDK LO potentials, the related account must be of record type Work Site</description>
        <errorConditionFormula>(ISPICKVAL (Sales_Area__c, &apos;1000&apos;) ||
ISPICKVAL (Sales_Area__c, &apos;4000&apos;)) &amp;&amp;
Customer__r.RecordType.Name != &apos;Work Site&apos; &amp;&amp;
$Setup.DisableAutomation__c.Disable_All_Validation_Rules__c = false</errorConditionFormula>
        <errorMessage>Potential for PDK BE or PDK LO must be attached to a Work SIte. The account you have tried to associate this Potential with is not a Work Site.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Reject_or_Convert_ME_LO_Potential</fullName>
        <active>true</active>
        <description>Make sure that if API tries to reject or convert ME or LO potentials it is not allowed. Already restricted via UI</description>
        <errorConditionFormula>AND( RecordType.Name = &apos;ME &amp; LO Potential&apos;, OR(ISPICKVAL( Status__c, &apos;Rejected&apos;), ISPICKVAL( Status__c, &apos;Converted to Opportunity&apos;))) &amp;&amp; $Setup.DisableAutomation__c.Disable_All_Validation_Rules__c = false</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You cannot reject or convert a potential belonging to ME or LO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Sales_Area</fullName>
        <active>true</active>
        <description>Sales Area must be PDK BE or LO DK or PDK LO - restricted via UI, rule is to check API inserts/updates</description>
        <errorConditionFormula>AND(NOT(ISPICKVAL ( Sales_Area__c, &apos;1000&apos;)),
NOT(ISPICKVAL ( Sales_Area__c, &apos;4000&apos;)),
NOT(ISPICKVAL ( Sales_Area__c, &apos;0540&apos;)))
&amp;&amp; $Setup.DisableAutomation__c.Disable_All_Validation_Rules__c = false</errorConditionFormula>
        <errorDisplayField>Sales_Area__c</errorDisplayField>
        <errorMessage>Invalid Sales Area</errorMessage>
    </validationRules>
