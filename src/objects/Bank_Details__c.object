    <fields>
        <fullName>Bank_Account_Transformed_Number__c</fullName>
        <description>This field is a transformation of the Bank Account field. A validation rule is using this field to validate the Back Account field.</description>
        <externalId>false</externalId>
        <formula>IF( 
OR( BEGINS( Bank_Account__c , &quot;4&quot;), 
BEGINS(Bank_Account__c, &quot;5&quot;)), 
LEFT( Bank_Account__c, 7) + 
LPAD(RIGHT(RIGHT( Bank_Account__c, LEN( Bank_Account__c)-7), 7), 7, &quot;0&quot;) 
, 
LEFT( Bank_Account__c, 6) + 
LPAD(RIGHT(RIGHT( Bank_Account__c, LEN( Bank_Account__c)-6), 8), 8, &quot;0&quot;) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Bank Account Transformed Number</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
