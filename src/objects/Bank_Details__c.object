    <validationRules>
        <fullName>Bank_Account_Checksum_Check_FI</fullName>
        <active>true</active>
        <description>If Bank Country is Finland then the Bank Account Number must pass a custom validation rule.</description>
        <errorConditionFormula>AND( ISPICKVAL( Bank_Country__c , &quot;FI&quot;), 
LEN( Bank_Account__c) &gt; 0, 
MOD( 
VALUE(MID(Bank_Account_Transformed_Number__c, 2, 1))+ 
VALUE(MID(Bank_Account_Transformed_Number__c, 4, 1))+ 
VALUE(MID(Bank_Account_Transformed_Number__c, 6, 1))+ 
VALUE(MID(Bank_Account_Transformed_Number__c, 8, 1))+ 
VALUE(MID(Bank_Account_Transformed_Number__c, 10, 1))+
VALUE(MID(Bank_Account_Transformed_Number__c, 12, 1))+
VALUE(MID(Bank_Account_Transformed_Number__c, 14, 1))+

IF( VALUE(MID(Bank_Account_Transformed_Number__c, 1, 1))*2 &gt; 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 1, 1))*2 - 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 1, 1))*2 
)+ 

IF( VALUE(MID(Bank_Account_Transformed_Number__c, 3, 1))*2 &gt; 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 3, 1))*2 - 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 3, 1))*2 
)+ 

IF( VALUE(MID(Bank_Account_Transformed_Number__c, 5, 1))*2 &gt; 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 5, 1))*2 - 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 5, 1))*2 
)+ 

IF( VALUE(MID(Bank_Account_Transformed_Number__c, 7, 1))*2 &gt; 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 7, 1))*2 - 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 7, 1))*2 
)+

IF( VALUE(MID(Bank_Account_Transformed_Number__c, 9, 1))*2 &gt; 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 9, 1))*2 - 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 9, 1))*2 
)+

IF( VALUE(MID(Bank_Account_Transformed_Number__c, 11, 1))*2 &gt; 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 11, 1))*2 - 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 11, 1))*2 
)+

IF( VALUE(MID(Bank_Account_Transformed_Number__c, 13, 1))*2 &gt; 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 13, 1))*2 - 9, 
VALUE(MID(Bank_Account_Transformed_Number__c, 13, 1))*2 
) 
,10) &lt;&gt; 0) &amp;&amp; $Setup.DisableAutomation__c.Disable_All_Validation_Rules__c = false</errorConditionFormula>
        <errorDisplayField>Bank_Account__c</errorDisplayField>
        <errorMessage>This is not a valid Bank Account Number in Finland</errorMessage>
    </validationRules>
