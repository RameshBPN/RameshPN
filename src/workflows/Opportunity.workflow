            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>When opp gets set to Closed Lost and Lead Identifier is in DK send Danish email</description>
        <formula>AND (ISPICKVAL( StageName , &apos;Closed Lost&apos;), Lead_Identifier_Country__c = &apos;DK&apos;,  Date_Closed_Lost_Feedback_Sent__c == null, $Setup.DisableAutomation__c.Disable_All_Non_OBM_Workflows__c =false)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
