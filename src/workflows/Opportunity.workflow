    <rules>
        <fullName>Check if Contract Renewal Opp Has 4000 or Korsler Products</fullName>
        <actions>
            <name>Set_Has_4000_or_Last_Mile_product_to_F</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Set_Has_Korsler_Product_to_False</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.no_of_products__c</field>
            <operation>lessThan</operation>
            <value>1</value>
        </criteriaItems>
        <description>Checks if opp has products - if not then set the field &quot;Has 4000 Products&quot; and field &quot;Has Korsler&quot; to false</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Contract Renewal Opportunity - Set Record Type to Visiting Customer</fullName>
        <actions>
            <name>Update_Opportunity_RT_to_Visiting_Cust</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Opportunity.RecordTypeId</field>
            <operation>equals</operation>
            <value>Contract Renewal Sending Opportunity</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Type</field>
            <operation>equals</operation>
            <value>Visiting Customer</value>
        </criteriaItems>
        <description>If the account record type is contract renewal sending customer and the opportunity type has been updated to visiting customer then update the record type to visitng customer.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
